<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="robAnchorpane" prefHeight="284.0" prefWidth="392.0" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.uniks.pioneers.controller.subcontroller.RobPlayerController">
    <Label alignment="CENTER" contentDisplay="RIGHT" layoutX="12.0" layoutY="20.0" prefHeight="17.0" prefWidth="373.0"
           text="Choose a player to rob from">
        <font>
          <Font size="27.0"/>
        </font>
    </Label>
    <Button fx:id="acceptButton" layoutX="163.0" layoutY="218.0" mnemonicParsing="false" prefHeight="39.0"
            prefWidth="66.0" text="OK">
        <font>
          <Font size="19.0"/>
        </font>
    </Button>
    <MenuButton fx:id="menuButton" accessibleRole="BUTTON" layoutX="99.0" layoutY="76.0" mnemonicParsing="false"
                prefHeight="65.0" prefWidth="209.0">
        <graphic>
          <HBox fx:id="selectedItemBox" alignment="CENTER" prefHeight="58.0" prefWidth="185.0"
                style="-fx-background-color: #ffff;"/>
        </graphic>
    </MenuButton>
    <ListView fx:id="playerListView" layoutX="104.0" layoutY="141.0" prefHeight="136.0" prefWidth="200.0"
              visible="false"/>
</AnchorPane>
